program object2;
type
 obj1ty = object
  public
   f1: int32;
 end;

 obj2ty = object(obj1ty)
  private
   f2: int32;
  public
   function sub1(par: int32): int32;
   property p2: int32 read f2 write f2;
 end;

var
 obj1: obj2ty;

function obj2ty.sub1(par: int32): int32;
begin
 result:= f1+p2+par;
end;

begin
 obj1.f1:= 100;
 obj1.p2:= 20;
 exitcode:= obj1.sub1(3);
end.
