program ord;
type
 enuty = (en_0,en_1,en_2);
var
 en1: enuty;
 ch1: char8;
 bo1: bool1;
begin
 exitcode:= system.ord(en_2) + system.ord('0')+system.ord(5)+system.ord(true); //56
 en1:= en_1; //1
 ch1:= '1';  //49
 bo1:= true; //1
 exitcode:= exitcode+system.ord(en1)+system.ord(ch1)+system.ord(bo1)+123-56-51;
end.