program classprop2;

type
 recty = record
  a,b: int32;
 end;
 tcla1 = class
  private
   fb: int32;
   fa: recty;
  public
   constructor create();
   destructor destroy();
 end;

 tcla = class(tcla1)
  private
  public
//{$internaldebug on}
   property a: int32 read fa.b write fa.a default 1+3;
   property b: int32 read fb write fb;
 end;

constructor tcla1.create();
begin
 fa.b:= 100;
end;

destructor tcla1.destroy();
begin
end;

var
 cla: tcla;
begin
 cla:= tcla.create();
 cla.a:= 23;
 exitcode:= cla.a + cla.fa.a;
 cla.destroy();
end.
