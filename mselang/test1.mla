program object4;

type
 objty = class
  f1: int32;
  constructor create(a: int32);
  destructor destroy();
  procedure test(const a: int32);
 end;

constructor objty.create(a: int32);
begin
 f1:= a;
end;

destructor objty.destroy();
begin
end;

procedure objty.test(a: int32);
begin
 f1:= f1 * a;
end;

var
 obj1: ^objty;
 obja: objty;
begin
{$internaldebug on}
 obja:= objty.create(123);
 obj1:= @obja;
 obj1^.test(10);
 exitcode:= obj1^.f1 div 10;
 obja.destroy();
end.
